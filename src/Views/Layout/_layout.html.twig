    {#
<script src="/js/libs/facebook.js" async="true" defer="defer"></script>
    <script src="//connect.facebook.net/{{ app.locale }}_FR/all.js" async="true" defer="defer"></script>
    #}
    {% if app.locale  != 'en' %}
    <script src="/js/plugins/bootstrap-select/i18n/defaults-{{ app.locale }}.js"></script>
