        $(function () {
            User.currentUser = {id:'{{ app.user.id }}', username: '{{ app.user.username }}'};
            $.material.init();
        });
    </script>
{% endblock %}
{% if not app.debug %}
    <script src="/js/chat/olark.js" type='text/javascript'></script>
    <script src="/js/compiled/{{ app.locale }}/olark_lang.js" type='text/javascript'></script>
