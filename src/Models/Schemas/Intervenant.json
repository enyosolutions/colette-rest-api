{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "/",
    "type": "object",
    "properties": {
        "sex": {
            "id": "sex",
            "type": "string"
        },
        "picture": {
            "id": "picture",
            "type": "string"
        },
        "firstname": {
            "id": "firstname",
            "type": "string"
        },
        "age": {
            "id": "age",
            "type": "string"
        },
        "diploma": {
            "id": "diploma",
            "type": "string"
        },
        "price": {
            "id": "price",
            "type": "integer"
        },
        "skills": {
            "id": "skills",
            "type": "array",
            "items" : {"type" : "string"}
        },
        "bio": {
            "id": "bio",
            "type": "string"
        },
        "address": {
            "id": "address",
            "type": "string"
        },
        "friendshipScore": {
            "id": "friendshipScore",
            "type": "integer"
        },
        "skillsScore": {
            "id": "skillsScore",
            "type": "integer"
        },
        "location": {
            "id": "location",
            "type": "object",
            "properties": {
                "lat": {
                    "id": "lat",
                    "type": "number"
                },
                "lng": {
                    "id": "lng",
                    "type": "number"
                }

            }
        }
    }
}